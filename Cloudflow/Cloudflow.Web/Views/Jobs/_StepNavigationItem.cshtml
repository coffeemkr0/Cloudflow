@model Cloudflow.Web.ViewModels.Jobs.ExtensionConfigurationViewModel

<li data-index="@Model.Index" role="presentation" class="stepNavigationItem @(Model.Active ? "active" : "")">
    <a href="#tab@(Model.Id)" role="tab" data-toggle="tab">
        @Model.Configuration.Name
        <i data-triggerid="@Model.Id" class="deleteStep glyphicon glyphicon-remove pull-right"></i>
    </a>
</li>