@model Cloudflow.Web.ViewModels.Jobs.ExtensionConfigurationViewModel

<li role="presentation" 
    class="conditionNavigationItem sortableNavigationItem @(Model.Active ? "active" : "")" 
    data-sortable-itemid="tab@(Model.Id)">
    <a href="#tab@(Model.Id)" role="tab" data-toggle="tab">
        @Model.Configuration.Name
        <i data-conditionid="@Model.Id" class="deleteCondition glyphicon glyphicon-remove pull-right"></i>
    </a>
</li>